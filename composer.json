{
    "name": "agentsib/jsonrpc",
    "description": "Json-RPC server and client",
    "type" : "library",
    "keywords": ["json", "rpc", "json-rpc", "api"],
    "homepage": "http://github.com/agentsib/jsonrpc",
    "authors": [
        {
            "name": "AgentSIB",
            "email": "agentsib@gmail.com",
            "homepage": "http://agentsib.ru"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "1.*",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-0": {"AgentSIB\\JsonRpc\\": "src/"}
    },
    "autoload-dev": {
        "psr-0": {"AgentSIB\\JsonRpc\\Tests": "tests/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
